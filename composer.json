{
    "name": "iksaku/fimeats",
    "description": "FIMEats Project.",
    "keywords": ["fimeats", "fime", "uanl"],
    "license": "GPLv3",
    "type": "project",
    "require": {
        "php": "^7.3",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "inertiajs/inertia-laravel": "^0.1",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "sausin/laravel-ovh": "^2.2",
        "symfony/yaml": "^4.2",
        "tightenco/ziggy": "^0.8"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.15",
        "fzaninotto/faker": "^1.4",
        "lorisleiva/laravel-deployer": "^0.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "routes": "@php artisan ziggy:generate resources/js/ziggy.js",
        "lint": "php-cs-fixer fix",
        "test:lint": "@lint --dry-run",
        "test": [
            "@test:lint"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
